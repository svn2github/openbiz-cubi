        else if ($dbtype == 'postgresql' || $dbtype == 'PDO_PGSQL')\par
            $sql = "select nextval('$sequenceName')";\par
        else if ($dbtype == 'informix' || $dbtype == 'PDO_INFORMIX')\par
            "select $sequenceName.nextval from systables where tabid=1";\par
        else\par
            throw new Exception("Error in generating new sequence: unsupported database type.");\par
        // execute sql to get the id\par
        $newId = $this->_getIdWithSql($conn, $sql);\par
        if ($newId === false)\par
            throw new Exception("Error in generating new sequence: unable to get a valid id.");\par

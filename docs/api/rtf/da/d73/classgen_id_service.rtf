            $sql = "SELECT * FROM ob_sysids WHERE TABLENAME='$tableName'";\par
            try\par
            \{\par
                $rs = $conn->query($sql);\par
            \}\par
            catch (Exception $e)\par
            \{\par
                throw new Exception("Error in query: " . $sql . ". " . $e->getMessage());\par
                return false;\par
            \}\par
            $row = $rs->fetch();\par
            unset($rs);\par
            list($tblname, $prefix, $idbody) = $row;\par
            if (!$row)\par
                throw new Exception("Error in generating new system id: '$tableName' is not in ob_sysids table.");\par
            if ($row)\par
            \{\par
                if ($idbody == null && $prefix) // idbody is empty, return false\par
                    throw new Exception("Error in generating new system id: ob_sysids table does not have a valid sequence for '$tableName'.");\par
            \}\par
            // try to update the table idbody column\par
            $sql = "UPDATE ob_sysids SET IDBODY=IDBODY+1 WHERE TABLENAME='$tableName' AND IDBODY=$idbody";\par
            try\par
            \{\par
                $rs = $conn->query($sql);\par
            \}\par
            catch (Exception $e)\par
            \{\par
                throw new Exception("Error in query: " . $sql . ". " . $e->getMessage());\par
                return false;\par
            \}\par
            if ($rs->rowCount() > 0)\par
            \{\par
                $idbody += 1;\par
                break;\par
            \}\par
        \}\par

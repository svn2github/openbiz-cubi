        $xmlArr = BizSystem::getXmlArray($chartXmlFile);\par
\par
        ob_clean();\par
        // get the chart section from config xml file\par
        foreach($xmlArr["BIZFORM_CHART"]["CHARTLIST"]["CHART"] as $chart)\par
        \{\par
            if (count($xmlArr["BIZFORM_CHART"]["CHARTLIST"]["CHART"]) == 1)\par
                $chart = $xmlArr["BIZFORM_CHART"]["CHARTLIST"]["CHART"];\par
            // try to match the chartName, if no chartName given, always draw the first chart defined in xml file\par
            if (($chartName && $chart["ATTRIBUTES"]["NAME"] == $chartName) || !$chartName)\par
            \{\par
                if ($chart["ATTRIBUTES"]["GRAPHTYPE"] == 'XY')\par
                \{\par
                    $this->xyGraphRender($bizDataObj, $chart);\par
                    break;\par
                \}\par
                if ($chart["ATTRIBUTES"]["GRAPHTYPE"] == 'Pie')\par
                \{\par
                    $this->pieGraphRender($bizDataObj, $chart);\par
                    break;\par
                \}\par
            \}\par
        \}\par

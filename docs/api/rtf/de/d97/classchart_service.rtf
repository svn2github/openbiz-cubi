        // get the value of the control that issues the call\par
        $chartName = BizSystem::clientProxy()->getFormInputs("__this");\par
\par
        // get the current UI bizobj\par
        $formObj = BizSystem::getObject($objName);    // get the existing bizform object\par
        $bizDataObj = $formObj->getDataObj();\par

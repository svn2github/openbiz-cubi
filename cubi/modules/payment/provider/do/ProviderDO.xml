<BizDataObj Name="ProviderDO" 
		DataPermControl="N"
		Class="BizDataObj" 
		DBName="Default" 
		Table="payment_provider" 
		SearchRule="" 
		SortRule="" 
		OtherSQLRule=""
		Uniqueness="" 
		Stateless="N" 
		IdGeneration="Identity" 
		CacheLifeTime="0" 
		CreateCondition="payment.Manage" 
		UpdateCondition="payment.Manage" 
		DeleteCondition="payment.Manage" >
	<BizFieldList>
		<BizField Name="Id"			
				Description="Id"	
				Column="id"    
				Required="N" 
				Type="Number"/>
				
		<BizField Name="driver"			
				Description="driver"	
				Column="driver"    
				Required="N" 
				Type="Text"/>
			
		<BizField Name="username"		
				required="Y" 
				Description="username"	
				Column="username" 
				Type="Text"
				Required="N" 
				 />	
		<BizField Name="password" required="Y" 
				Description="password"	
				Column="password" 
				Type="Text"
				Required="N" 
				 />
		<BizField Name="site_url"	 
				Description="site_url"	
				Column="site_url" 
				Type="Text"
				Required="N" 
				 />

	 	<BizField Name="type"	
				Description="type"	
				Column="type" 
				Type="Text"
				Required="N" 
				 />
		<BizField Name="name"		
				Description="Name"	
				Column="name" 
				Type="Text"
				Required="N" 
				 />				 
		<BizField Name="description"		
				Description="Description"	
				Column="description" 
				Type="Text"
				Required="N" 
				 />
	
		<BizField Name="status"		
				Description="Status"	
				Column="status" 
				Type="Number"
				Required="N" 
				 />
		<BizField Name="update_by"	
				Description="Update By"	
				Column="update_by"	
				Type="Number"	
				ValueOnCreate="{@profile:Id}"	
				ValueOnUpdate="{@profile:Id}"/>
		<BizField Name="update_time"	
				Description="Update Time"	
				Column="update_time"	
				Type="Datetime"	
				ValueOnCreate="{date('Y-m-d H:i:s')}"	
				ValueOnUpdate="{date('Y-m-d H:i:s')}"/>
		<BizField Name="create_by"	
				Description="Create By"	
				Column="create_by"	
				Type="Number"	
				ValueOnCreate="{@profile:Id}"/>
		<BizField Name="create_time"	
				Description="Create Time"	
				Column="create_time"	
				Type="Datetime"	
				ValueOnCreate="{date('Y-m-d H:i:s')}"/>
	</BizFieldList>
</BizDataObj>
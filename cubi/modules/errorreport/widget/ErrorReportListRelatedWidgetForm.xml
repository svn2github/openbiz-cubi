<EasyForm Name="ErrorReportListRelatedWidgetForm" 
			Class="PickerForm"			  
			FormType="List" 			
			jsClass="Openbiz.TableForm" 
			BizDataObj="errorreport.do.ErrorReportRefDO" 
			PageSize="-1"  
			DefaultForm="Y" 
			TemplateEngine="Smarty" 
			TemplateFile="element_listform_lite.tpl.html" 
			MessageFile="ErrorReport.ini" 
			Access="errorreport.Access">	
    <DataPanel>           
            
        			
		<Element Name="fld_Id" 
        			Class="common.element.ColumnTitle" 
        			FieldName="Id" 
        			Label="ID" 
        			Sortable="Y" 
        			AllowURLParam="N" 
        			Link="javascript:;" />   
		<Element Name="fld_share" Class="ColumnShare" 
				MyPrivateImg="{RESOURCE_URL}/errorreport/images/icon_errorreport_private.gif"
				MySharedImg="{RESOURCE_URL}/errorreport/images/icon_errorreport_shared.gif" 
				MyAssignedImg="{RESOURCE_URL}/errorreport/images/icon_errorreport_assigned.gif"
				MyDistributedImg="{RESOURCE_URL}/errorreport/images/icon_errorreport_distributed.gif" 
				GroupSharedImg="{RESOURCE_URL}/errorreport/images/icon_errorreport_shared_group.gif"
				OtherSharedImg="{RESOURCE_URL}/errorreport/images/icon_errorreport_shared_other.gif"
				FieldName="create_by" 
				Label="Share"  
				Sortable="Y" 
				AllowURLParam="N" 
				Translatable="N" 
				OnEventLog="N" 
				Link="javascript:;">
		</Element>
   
    
		<Element Name="fld_name" 
        			Class="ColumnText" 
        			FieldName="name" 
        			Label="Name"         			 
        			Sortable="Y" 
        			MaxLength="15"
        			Link="{APP_INDEX}/errorreport/error_report_detail/{@:Elem[fld_Id].Value}"        			
        			 /> 
        	
   
    
		<Element Name="fld_description" 
        			Class="ColumnText" 
        			FieldName="description" 
        			Label="Description" 
        			MaxLength="15"        			 
        			Sortable="Y"
        			 />        			
        	
   
    
		<Element Name="fld_error_data" 
        			Class="ColumnText" 
        			FieldName="error_data" 
        			Label=" Data" 
        			MaxLength="15"        			 
        			Sortable="Y"
        			 />        			
        	
   
    
		<Element Name="fld_php_version" 
        			Class="ColumnText" 
        			FieldName="php_version" 
        			Label=" Version" 
        			MaxLength="15"        			 
        			Sortable="Y"
        			 />        			
        	
   
    
		<Element Name="fld_php_extension" 
        			Class="ColumnText" 
        			FieldName="php_extension" 
        			Label=" Extension" 
        			MaxLength="15"        			 
        			Sortable="Y"
        			 />        			
        	
   
    
    
    
    
    
    
    
    
    
    
    
		<Element Name="fld_color" 
				Class="ColumnStyle" 
				FieldName="type_color" 
				Label="Type"  
				Sortable="Y" 
				AllowURLParam="N" 
				Translatable="N" 
				OnEventLog="N" />
				
		<Element Name="fld_type" 
				Class="ColumnText"  
				Style="line-height:24px;" 
				FieldName="type_name" 
				Label="Type"  
				Sortable="Y" 
				AllowURLParam="N" 
				Translatable="N" 
				OnEventLog="N" />						        
  
    </DataPanel>
    <ActionPanel /> 
    <NavPanel />     
</EasyForm>
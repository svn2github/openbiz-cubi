		<Element Name="fld_oauth_class" Class="InputText" FieldName="oauth_class" Label="Oauth Class" />
		<Element Name="fld_oauth_secret" Class="CKEditor"  mode="adv"  Config="resize_maxWidth:640,resize_minWidth:640,resize_minHeight:300" width="640" height="300" FieldName="oauth_secret" Label="Oauth Secret"   />            
		<Element Name="fld_oauth_rawdata" Class="CKEditor"  mode="adv"  Config="resize_maxWidth:640,resize_minWidth:640,resize_minHeight:300" width="640" height="300" FieldName="oauth_rawdata" Label="Oauth Rawdata"   />            

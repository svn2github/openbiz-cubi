<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="UserTokenEditForm" Class="EasyForm" FormType="Edit" jsClass="jbForm" Title="Edit User Token" Description="" BizDataObj="oauth.do.UserTokenDO" DefaultForm="Y" TemplateEngine="Smarty" TemplateFile="detail.tpl" EventName="" MessageFile="">
    <DataPanel>
		<Element Name="fld_Id" Class="Hidden" FieldName="Id" Label="Id" AllowURLParam="Y" CssClass="input" CssErrorClass="input_error"/>

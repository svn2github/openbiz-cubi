<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="OauthProviderDetailForm" Class="EasyForm" FormType="" jsClass="jbForm" Title="Oauth Provider Detail" Description="" BizDataObj="oauth.do.OauthProviderDO" TemplateEngine="Smarty" TemplateFile="detail_elementset.tpl" EventName="" MessageFile="">
    <DataPanel>
		<Element Name="fld_Id" ElementSet="General" Hidden="Y" Class="LabelText" FieldName="Id" Label="Id" AllowURLParam="Y"/>
       	<Element Name="fld_user_id" ElementSet="General" Class="LabelText" FieldName="user_id" Label="User Id" AllowURLParam="N"/>
       	<Element Name="fld_type" ElementSet="General" Class="LabelText" FieldName="type" Label="Type" AllowURLParam="N"/>
       	<Element Name="fld_key" ElementSet="General" Class="LabelText" FieldName="key" Label="Key" AllowURLParam="N"/>
       	<Element Name="fld_value" ElementSet="General" Class="LabelText" FieldName="value" Label="Value" AllowURLParam="N"/>
       	<Element Name="fld_create_by" Class="LabelText" ElementSet="Miscellaneous" FieldName="create_by" Label="Create By" Text="{BizSystem::GetProfileName(@:Elem[fld_create_by].Value)}" AllowURLParam="N"/>       	
       	<Element Name="fld_create_time" Class="LabelText" ElementSet="Miscellaneous" FieldName="create_time" Label="Create Time"  AllowURLParam="N"/>       	
       	<Element Name="fld_update_by" Class="LabelText" ElementSet="Miscellaneous" FieldName="update_by" Label="Update By" Text="{BizSystem::GetProfileName(@:Elem[fld_update_by].Value)}" AllowURLParam="N"/>       	
       	<Element Name="fld_update_time" Class="LabelText" ElementSet="Miscellaneous" FieldName="update_time" Label="Update Time"  AllowURLParam="N"/>       	
    </DataPanel>
    <ActionPanel>       
        <Element Name="btn_new" Class="Button" Text="Add" CssClass="button_gray_add" Description="new record (Insert)">
			<EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(oauth.form.OauthProviderNewForm)"  ShortcutKey="Insert" ContextMenu="New" />
        </Element>          
        <Element Name="btn_edit" Class="Button" Text="Edit" CssClass="button_gray_m" Description="edit record (Ctrl+E)">
			<EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(oauth.form.OauthProviderEditForm,{@:Elem[fld_Id].Value})"  ShortcutKey="Ctrl+E" ContextMenu="Edit" />
        </Element>
		<Element Name="btn_copy" Class="Button" Text="Copy" CssClass="button_gray_m" Description="copy record (Ctrl+C)">
            <EventHandler Name="onclick" Event="onclick" EventLogMsg="" Function="CopyRecord({@:Elem[fld_Id].Value})" RedirectPage="form=oauth.form.OauthProviderCopyForm&amp;fld:Id={@:Elem[fld_Id].Value}" ShortcutKey="Ctrl+C" ContextMenu="Copy"/>
        </Element> 
        <Element Name="btn_delete" Class="Button" Text="Delete" CssClass="button_gray_m" Description="delete record (Delete)">
            <EventHandler Name="del_onclick" Event="onclick" EventLogMsg="" Function="DeleteRecord({@:Elem[fld_Id].Value})"  RedirectPage="form=oauth.form.OauthProviderListForm" ShortcutKey="Ctrl+Delete" ContextMenu="Delete" />
        </Element>
        <Element Name="btn_cancel" Class="Button" Text="Back" CssClass="button_gray_m">
            <EventHandler Name="btn_cancel_onclick" Event="onclick" Function="SwitchForm()"  ShortcutKey="Escape" ContextMenu="Cancel" />
        </Element>         
    </ActionPanel> 
    <NavPanel>
    </NavPanel> 
    <SearchPanel>
    </SearchPanel>
</EasyForm>

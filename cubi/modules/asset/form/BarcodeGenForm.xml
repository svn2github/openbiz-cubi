<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="BarcodeGenForm" 
			Class="BarcodeGenForm"			  
			FormType="List" 
			jsClass="Openbiz.TableForm" 
			Icon="{RESOURCE_URL}/asset/images/icon_mod_asset_list.png"
			Title="Batch Barcode Generate" 
			Description="This tool will can help you easily create batch of barcodes" 
			BizDataObj="asset.do.AssetDO" 
			PageSize="15" 
			DefaultForm="Y" 
			TemplateEngine="Smarty" 
			TemplateFile="form_grid_barcode.tpl.html" 
			EventName="Asset" 
			MessageFile="Asset.ini" 
			Access="asset.Access">
	<DataPanel>
            
				     
<!-- 
		<Element Name="fld_Id" 
        			Class="Hidden" 
        			FieldName="Id" 
        			Label="ID" 
        			Sortable="Y" 
        			Hidden="Y"
        			AllowURLParam="N" 
        			Link="javascript:;" />  
 -->
         	
   		<Element Name="fld_barcode" 
        			Class="asset.element.ColumnBarcode" 
        			FieldName="barcode" 
        			Label="Barcode"         			        			
        			Sortable="Y"
        			Scale="2"
        			Height="70"
        			Width="216"
        			 /> 
       
  
	</DataPanel>
	<SearchPanel>
		<Element Name="btn_spacer" Class="Spacer" Width="10" />
	</SearchPanel>
    <ActionPanel>    	
        
        <Element Name="btn_regenerate" 
        			Class="Button" 
        			Text="Regenerate" 
        			Description="Regenerate Barcodes"
        			CssClass="button_gray_w">
            <EventHandler Name="btn_regenerate_onclick" 
            				Event="onclick" 
            				EventLogMsg="" 
            				Function="UpdateForm()" 
            				/>
        </Element>
    </ActionPanel> 
    <NavPanel>
    	

        
        <!-- Page Size Selector -->
        <Element  Name="pagesize_selector" 
        			Class="PagesizeSelector" 
        			Text="{@:m_Range}" 
        			Label="Quailty" 
        			CssClass="input_select" 
        			CssFocusClass="input_select_focus">
            <EventHandler Name="btn_pagesize_selector_onchange" 
            				Event="onchange" 
            				Function="SetPageSize(pagesize_selector)"/>
        </Element> 

    </NavPanel> 
    
</EasyForm>
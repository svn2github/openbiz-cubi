<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="LicenseNewForm" Class="EasyForm" FormType="New" jsClass="jbForm" Title="New License" Description="" BizDataObj="license.do.LicenseDO" TemplateEngine="Smarty" TemplateFile="detail.tpl" EventName="" MessageFile="">
    <DataPanel>
        <Element Name="fld_sku" Class="InputText" FieldName="sku" Label="Sku"   AllowURLParam="N" CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
        <Element Name="fld_contact_id" Class="InputText" FieldName="contact_id" Label="Contact Id"   AllowURLParam="N" CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
        <Element Name="fld_policy_id" Class="InputText" FieldName="policy_id" Label="Policy Id"   AllowURLParam="N" CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
        <Element Name="fld_activation_code" Class="InputText" FieldName="activation_code" Label="Activation Code"   AllowURLParam="N" CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
        <Element Name="fld_status" Class="InputText" FieldName="status" Label="Status" DefaultValue="1"  AllowURLParam="N" CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
        <Element Name="fld_description" Class="InputText" FieldName="description" Label="Description"   AllowURLParam="N" CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
    			<Element Name="fld_start_time" Class="InputDatetime" DateFormat="%Y-%m-%d %H:%M:%S" FieldName="start_time" Label="Start Time"  CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
    			<Element Name="fld_end_time" Class="InputDatetime" DateFormat="%Y-%m-%d %H:%M:%S" FieldName="end_time" Label="End Time"  CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
        <Element Name="fld_max_use" Class="InputText" FieldName="max_use" Label="Max Use" DefaultValue="1"  AllowURLParam="N" CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
        <Element Name="fld_cur_use" Class="InputText" FieldName="cur_use" Label="Cur Use" DefaultValue="0"  AllowURLParam="N" CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
        </DataPanel>
    <ActionPanel>
        <Element Name="btn_save" Class="Button" Text="Save" CssClass="button_gray_m">
            <EventHandler Name="save_onclick" EventLogMsg="" Event="onclick" Function="InsertRecord()" RedirectPage="form=license.form.LicenseDetailForm&amp;fld:Id={@license.do.LicenseDO:Field[Id].Value}"  ShortcutKey="Ctrl+Enter" ContextMenu="Save" />
        </Element>
        <Element Name="btn_cancel" Class="Button" Text="Cancel" CssClass="button_gray_m">
            <EventHandler Name="cancel_onclick" Event="onclick" Function="SwitchForm()"  ShortcutKey="Escape" ContextMenu="Cancel"/>
        </Element>
    </ActionPanel> 
    <NavPanel>
    </NavPanel> 
    <SearchPanel>
    </SearchPanel>
</EasyForm>

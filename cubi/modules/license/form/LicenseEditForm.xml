<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="LicenseEditForm" Class="EasyForm" FormType="Edit" jsClass="jbForm" Title="Edit License" Description="" BizDataObj="license.do.LicenseDO" DefaultForm="Y" TemplateEngine="Smarty" TemplateFile="detail.tpl" EventName="" MessageFile="">
    <DataPanel>
		<Element Name="fld_Id" Class="Hidden" FieldName="Id" Label="Id" AllowURLParam="Y" CssClass="input" CssErrorClass="input_error"/>
		<Element Name="fld_sku" Class="InputText" FieldName="sku" Label="Sku" />
		<Element Name="fld_contact_id" Class="InputText" FieldName="contact_id" Label="Contact Id" />
		<Element Name="fld_policy_id" Class="InputText" FieldName="policy_id" Label="Policy Id" />
		<Element Name="fld_activation_code" Class="InputText" FieldName="activation_code" Label="Activation Code" />
		<Element Name="fld_status" Class="InputText" FieldName="status" Label="Status" />
		<Element Name="fld_description" Class="InputText" FieldName="description" Label="Description" />
		<Element Name="fld_start_time" Class="InputDatetime" DateFormat="%Y-%m-%d %H:%M:%S" FieldName="start_time" Label="Start Time"   CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
		<Element Name="fld_end_time" Class="InputDatetime" DateFormat="%Y-%m-%d %H:%M:%S" FieldName="end_time" Label="End Time"   CssClass="input_text" CssErrorClass="input_text_error" CssFocusClass="input_text_focus"/>
		<Element Name="fld_max_use" Class="InputText" FieldName="max_use" Label="Max Use" />
		<Element Name="fld_cur_use" Class="InputText" FieldName="cur_use" Label="Cur Use" />
    </DataPanel>
    <ActionPanel>
        <Element Name="btn_save" Class="Button" Text="Save" CssClass="button_gray_m">
            <EventHandler Name="save_onclick" Event="onclick" EventLogMsg=""  Function="UpdateRecord()" RedirectPage="form=license.form.LicenseDetailForm&amp;fld:Id={@license.do.LicenseDO:Field[Id].Value}" ShortcutKey="Ctrl+Enter" ContextMenu="Save" />
        </Element>
        <Element Name="btn_cancel" Class="Button" Text="Cancel" CssClass="button_gray_m">
            <EventHandler Name="btn_cancel_onclick" Event="onclick" Function="SwitchForm()"  ShortcutKey="Escape" ContextMenu="Cancel" />
        </Element>
    </ActionPanel> 
    <NavPanel>
    </NavPanel> 
    <SearchPanel>
    </SearchPanel>
</EasyForm>

<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="AccountIndustryListWidgetForm" 
			Class="EasyForm"			  
			FormType="List" 			
			jsClass="Openbiz.TableForm" 
			BizDataObj="account.Industry.do.AccountIndustryDO" 
			PageSize="-1"  
			DefaultForm="Y" 
			TemplateEngine="Smarty" 
			TemplateFile="element_listform_lite.tpl.html" 
			MessageFile="AccountIndustry.ini" 
			Access="account.Access">	
    <DataPanel>           
            
		<Element Name="row_selections" 
        			Class="RowCheckbox"  
        			Label="" 
        			FieldName="Id"/>
		<Element Name="fld_Id" 
        			Class="common.element.ColumnTitle" 
        			FieldName="Id" 
        			Label="ID" 
        			Sortable="Y" 
        			AllowURLParam="N" 
        			Link="javascript:;" />   
		<Element Name="fld_share" Class="ColumnShare" 
				MyPrivateImg="{RESOURCE_URL}/account/images/icon_account_private.gif"
				MySharedImg="{RESOURCE_URL}/account/images/icon_account_shared.gif" 
				MyAssignedImg="{RESOURCE_URL}/account/images/icon_account_assigned.gif"
				MyDistributedImg="{RESOURCE_URL}/account/images/icon_account_distributed.gif" 
				GroupSharedImg="{RESOURCE_URL}/account/images/icon_account_shared_group.gif"
				OtherSharedImg="{RESOURCE_URL}/account/images/icon_account_shared_other.gif"
				FieldName="create_by" 
				Label="Share"  
				Sortable="Y" 
				AllowURLParam="N" 
				Translatable="N" 
				OnEventLog="N" 
				Link="javascript:;">
		</Element>
   
    
		<Element Name="fld_name" 
        			Class="ColumnText" 
        			FieldName="name" 
        			Label="Name"         			 
        			Sortable="Y" 
        			MaxLength="15"
        			Link="{APP_INDEX}/account/account_industry_detail/{@:Elem[fld_Id].Value}"        			
        			 /> 
        	
   
    
		<Element Name="fld_description" 
        			Class="ColumnText" 
        			FieldName="description" 
        			Label="Description" 
        			MaxLength="15"        			 
        			Sortable="Y"
        			 />        			
        	
   
    
   
    
   
    
   
    
   
    
		<Element Name="fld_status" 
					Class="ColumnBool" 
					FieldName="status" 
					Label="Status"  
					Sortable="Y" 
					AllowURLParam="N" 
					Translatable="N" 
					OnEventLog="N" 
					Link="javascript:;">
		</Element>
   
    
   
    
   
    
   
    
   
    
  
    </DataPanel>
    <ActionPanel /> 
    <NavPanel />     
</EasyForm>
<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="AccountIndustryDetailForm" Class="EasyForm" FormType="Detail" jsClass="Openbiz.Form" Icon="{RESOURCE_URL}/account/images/icon_mod_account_industry_detail.png" Title="Account Industry Information Detail" Description="Account Industry" BizDataObj="account.industry.do.AccountIndustryDO" TemplateEngine="Smarty" TemplateFile="form_detail_adv.tpl.html" MessageFile="AccountIndustry.ini" Access="account.Access">
  <DataPanel>
    <Element Name="fld_Id" Class="LabelText" ElementSet="General" Hidden="Y" FieldName="Id" Label="Id" AllowURLParam="Y"/>
    <Element Name="fld_name" ElementSet="General" Class="LabelText" FieldName="name" Label="Name" AllowURLParam="N"/>
    <Element Name="fld_description" ElementSet="General" Class="LabelTextarea" FieldName="description" Label="Description" AllowURLParam="N"/>
    <Element Name="fld_status" Class="LabelBool" ElementSet="General" FieldName="status" Label="Status" Sortable="Y" AllowURLParam="N" Translatable="N" OnEventLog="N" Link="javascript:;">
		</Element>
    <Element Name="fld_related_account" TabSet="Extra Information" ElementSet="Account" Class="FormElement" FormReference="account.widget.AccountListWidgetForm" FieldName="" Label="" AllowURLParam="N" Access="account.Access"/>
    <Element Name="btn_manage_account" Hidden="{@:m_CanUpdateRecord=='1'?'N':'Y'}" TabSet="Extra Information" ElementSet="Account" Class="Button" CssClass="button_gray_w" Text="Manage" Style="color:#666666;margin-left:5px;margin-top:2px;" Access="account.Access">
      <EventHandler Name="btn_manage_account_onclick" Event="onclick" Function="SwitchForm(account.industry.form.AccountIndustryEditAccountForm,{@:Elem[fld_Id].Value})"/>
    </Element>
    <Element Name="fld_update_by" Class="LabelText" ElementSet="Miscellaneous" FieldName="update_by" Label="Update By" Text="{BizSystem::GetProfileName(@:Elem[fld_update_by].Value)}" AllowURLParam="N"/>
    <Element Name="fld_update_time" Class="LabelText" ElementSet="Miscellaneous" FieldName="update_time" Label="Update Time" AllowURLParam="N"/>
    <Element Name="fld_create_by" Class="LabelText" ElementSet="Miscellaneous" FieldName="create_by" Label="Create By" Text="{BizSystem::GetProfileName(@:Elem[fld_create_by].Value)}" AllowURLParam="N"/>
    <Element Name="fld_create_time" Class="LabelText" ElementSet="Miscellaneous" FieldName="create_time" Label="Create Time" AllowURLParam="N"/>
  </DataPanel>
  <ActionPanel>
    <Element Name="btn_share" Hidden="{@:m_CanUpdateRecord=='1'?'N':'Y'}" Class="Button" Text="Share" CssClass="button_gray_share" Description="Share">
      <EventHandler Name="btn_update_onclick" Event="onclick" Function="LoadDialog(common.form.DataSharingForm,{@:Elem[fld_Id].Value})"/>
    </Element>
    <Element Name="btn_spacer_for_share" Hidden="{@:m_CanUpdateRecord=='1'?'N':'Y'}" Class="Spacer" Width="10"/>
    <Element Name="btn_new" Class="Button" Text="Add" CssClass="button_gray_add" Description="New record (Insert)">
      <EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(account.industry.form.AccountIndustryNewForm)" ShortcutKey="Insert" ContextMenu="New"/>
    </Element>
    <Element Name="btn_edit" Class="Button" Text="Edit" CssClass="button_gray_m" Description="Edit record (Ctrl+E)">
      <EventHandler Name="btn_new_onclick" Event="onclick" Function="SwitchForm(account.industry.form.AccountIndustryEditForm,{@:Elem[fld_Id].Value})" ShortcutKey="Ctrl+E" ContextMenu="Edit"/>
    </Element>
    <Element Name="btn_copy" Class="Button" Text="Copy" CssClass="button_gray_m" Description="Copy record (Ctrl+C)">
      <EventHandler Name="onclick" Event="onclick" EventLogMsg="" Function="CopyRecord({@:Elem[fld_Id].Value})" RedirectPage="form=account.industry.form.AccountIndustryCopyForm&amp;fld:Id={@:Elem[fld_Id].Value}" ShortcutKey="Ctrl+C" ContextMenu="Copy"/>
    </Element>
    <Element Name="btn_delete" Class="Button" Text="Delete" CssClass="button_gray_m" Description="Delete record (Delete)">
      <EventHandler Name="del_onclick" Event="onclick" EventLogMsg="" Function="DeleteRecord({@:Elem[fld_Id].Value})" RedirectPage="{APP_INDEX}/account/account_industry_manage" ShortcutKey="Ctrl+Delete" ContextMenu="Delete"/>
    </Element>
    <Element Name="btn_cancel" Class="LabelBack" Text="Back" CssClass="button_gray_m" Link="account/account_industry_manage"/>
  </ActionPanel>
  <NavPanel>
    </NavPanel>
  <SearchPanel>
    </SearchPanel>
</EasyForm>

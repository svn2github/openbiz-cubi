<EasyForm Name="SmsProviderDetailForm" 
			Class="EasyForm" 
			FormType="Detail" 
			jsClass="Openbiz.Form" 
			Icon="{RESOURCE_URL}/sms/images/icon_mod_sms_provider_detail.png"
			Title="sms_provider Detail" 
			Description="Sms Provider Description" 
			BizDataObj="sms.do.SmsProviderDO" 
			TemplateEngine="Smarty" 
			TemplateFile="form_detail.tpl.html" 
			MessageFile="SmsProvider.ini" 
			Access="sms.Access">
    <DataPanel>

       	<Element Name="fld_Id" 
       				Class="LabelText" 
       				ElementSet="General"
       				Hidden="Y" 
       				FieldName="Id" 
       				Label="Id"  
       				AllowURLParam="Y"/>  
        				         				  				      				
  	<Element Name="fld_password" 
       				ElementSet="General"
       				Class="LabelText" 
       				FieldName="password" 
       				Label="password" 
       				AllowURLParam="N"
       				/>
    
       								   	

		

       	<Element Name="fld_username" 
       				ElementSet="General"
       				Class="LabelText" 
       				FieldName="username" 
       				Label="Username" 
       				AllowURLParam="N"
       				/>
    
       								   	

		
		<Element Name="fld_site_url" 
					ElementSet="General" 
        			Class="LabelText" 
        			FieldName="site_url" 
        			Label="site_url" 
        			MaxLength="15"        			 
        			AllowURLParam="N"
        			 />  
        				         				  				      				
       	<Element Name="fld_description" 
       				ElementSet="General"
       				Class="LabelTextarea" 
       				FieldName="description" 
       				Label="Description" 
       				AllowURLParam="N"
					/>
        				         				  				      				
        				         				  				      				
        				         				  				      				
        				         				  				      				
        				         				  				      				
		<Element Name="fld_status" 
					Class="LabelBool" 
					ElementSet="General"
					FieldName="status" 
					Label="Status"  
					Sortable="Y" 
					AllowURLParam="N" 
					Translatable="N" 
					OnEventLog="N" 
					Link="javascript:;">				
		</Element>
        				         				  				      				
        				         				  				      				
        				         				  				      				
        				         				  				      				
        				         				  				      				

		<Element Name="fld_changelog" 
				TabSet="Extra Information"  
				Access="changelog.access"   
				ElementSet="Change Log" 
				Class="FormElement" 
				FormReference="changelog.widget.ChangeLogWidgetForm" 
				FieldName="" 
				Label="" 
				AllowURLParam="N" />

       	<Element Name="fld_update_by" 
       				Class="LabelText" 
       				ElementSet="Miscellaneous" 
       				FieldName="update_by" 
       				Label="Update By" 
       				Text="{BizSystem::GetProfileName(@:Elem[fld_update_by].Value)}" 
       				AllowURLParam="N"
       				 />  
       				   	
       	<Element Name="fld_update_time" 
       				Class="LabelText" 
       				ElementSet="Miscellaneous" 
       				FieldName="update_time" 
       				Label="Update Time"  
       				AllowURLParam="N"
       				 />       	

					
       	<Element Name="fld_create_by" 
       				Class="LabelText" 
       				ElementSet="Miscellaneous" 
       				FieldName="create_by" 
       				Label="Create By" 
       				Text="{BizSystem::GetProfileName(@:Elem[fld_create_by].Value)}" 
       				AllowURLParam="N"
       				 />  
       				   	
       	<Element Name="fld_create_time" 
       				Class="LabelText" 
       				ElementSet="Miscellaneous" 
       				FieldName="create_time" 
       				Label="Create Time"  
       				AllowURLParam="N"
       				 />       	

					
    </DataPanel>
    <ActionPanel>  

        
        <Element Name="btn_edit" 
        			Class="Button" 
        			Text="Edit" 
        			CssClass="button_gray_m" 
        			Description="Edit record (Ctrl+E)">
			<EventHandler Name="btn_new_onclick" 
							Event="onclick" 
							Function="SwitchForm(sms.form.SmsProviderEditForm,{@:Elem[fld_Id].Value})"  
							ShortcutKey="Ctrl+E" 
							ContextMenu="Edit" />
        </Element>

        <Element Name="btn_cancel" 
        			Class="LabelBack" 
        			Text="Back" 
        			CssClass="button_gray_m"
        			Link="sms/sms_provider_manage" />        
    </ActionPanel> 
    <NavPanel>
    </NavPanel> 
    <SearchPanel>
    </SearchPanel>
</EasyForm>
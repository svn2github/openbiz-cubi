<BizDataObj Name="LogDO" 
		Description="Sms log" 
		DataPermControl="N"
		Class="BizDataObj" 
		DBName="Default" 
		Table="sms_log" 
		SearchRule="" 
		SortRule="[Id] DESC"
		OtherSQLRule=""
		Uniqueness="" 
		Stateless="N" 
		IdGeneration="Identity" 
		CacheLifeTime="0" 
		CreateCondition="sms.Manage" 
		UpdateCondition="sms.Manage" 
		DeleteCondition="sms.Manage" >
	<BizFieldList>
		<BizField Name="Id"			
				Description="Id"	
				Column="id"    
				Required="N" 
				Type="Number"/>
				
		<BizField Name="sender"			
				Description="sender"	
				Column="Sender"    
				Required="N" 
				Type="Number"
				ValueOnCreate="{@profile:Id}"
				/>	
				
		<BizField Name="tasklist_id"			
				Description="tasklist_id"	
				Column="tasklist_id"    
				Required="Y" 
				Type="Number"
				/>	
		<BizField Name="queue_id"			
				Description="queue_id"	
				Column="queue_id"    
				Required="Y" 
				Type="Number"
				/>
		<BizField Name="provider"			
				Description="provider"	
				Column="provider"    
				Required="N" 
				Type="Text"
				/>				
		<BizField Name="mobile"		
				Description="mobile"	
				Column="mobile" 
				Type="Text"
				Required="N" 
				 />
		<BizField Name="content"		
				Description="Content"	
				Column="content" 
				Type="Text"
				Required="N" 
				 />
		<BizField Name="status"		
				Description="Status"	
				Column="status" 
				Type="Text"
				Required="N" 
				 />
		<BizField Name="sent_time"		
				Description="Sent Time"	
				Column="sent_time" 
				Type="Datetime"
				Required="N" 
				 />
			
		<BizField Name="create_time"	
				Description="Create Time"	
				Column="create_time"	
				Type="Datetime"	
				ValueOnCreate="{date('Y-m-d H:i:s')}"/>
	</BizFieldList>
	<TableJoins>
				    	
	</TableJoins>
	
</BizDataObj>
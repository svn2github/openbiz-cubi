<BizDataObj Name="LogDO" 
		Description="Sms log" 
		DataPermControl="N"
		Class="BizDataObj" 
		DBName="Default" 
		Table="sms_log" 
		SearchRule="" 
		SortRule="[Id] DESC"
		OtherSQLRule=""
		Uniqueness="" 
		Stateless="N" 
		IdGeneration="Identity" 
		CacheLifeTime="0"  >
	<BizFieldList>
		<BizField Name="Id"			
				Description="Id"	
				Column="id"    
				Required="N" 
				Type="Number"/>


		<BizField Name="provider_id"			
				Column="provider_id"    				
				/>		
		<BizField Name="provider_name"			
				Column="name"
				Join="JoinProvider"    				
				/>		
		<BizField Name="mobile"		
				Description="mobile"	
				Column="mobile" 
				Type="Text"
				Required="N" 
				 />
		<BizField Name="content"		
				Description="Content"	
				Column="content" 
				Type="Text"
				Required="N" 
				 />
		<BizField Name="schedule"							
				Column="schedule" 	
				ValueOnCreate="{date('Y-m-d H:i:s')}"							 
				 />	
		<BizField Name="sent_time"		
				Description="Sent Time"	
				Column="sent_time" 
				Type="Datetime"
				Required="N" 
				 />
			
	</BizFieldList>
	<TableJoins>
    	<Join Name="JoinProvider" Table="sms_provider" Column="id" ColumnRef="provider_id" JoinType="LEFT JOIN"/>
  	</TableJoins>
	
</BizDataObj>
<EasyForm Name="SmsQueueListWidgetForm" 
			Class="EasyForm"			  
			FormType="List" 			
			jsClass="Openbiz.TableForm" 
			BizDataObj="sms.do.SmsQueueDO" 
			PageSize="-1"  
			DefaultForm="Y" 
			TemplateEngine="Smarty" 
			TemplateFile="element_listform_lite.tpl.html" 
			MessageFile="" 
			Access="System.Administer_System">	
    <DataPanel>           
            
		<Element Name="row_selections" 
        			Class="RowCheckbox"  
        			Label="" 
        			FieldName="Id"/>
        			
		<Element Name="fld_Id" 
        			Class="common.element.ColumnTitle" 
        			FieldName="Id" 
        			Label="ID" 
        			Sortable="Y" 
        			AllowURLParam="N" 
        			Link="javascript:;" />   
   
    
		<Element Name="fld_sender" 
        			Class="ColumnText" 
        			FieldName="sender" 
        			Label="Sender"         			 
        			Sortable="Y" 
        			MaxLength="15"
        			Link="{APP_INDEX}/sms/sms_queue_detail/{@:Elem[fld_Id].Value}"        			
        			 /> 
        	
   
    
		<Element Name="fld_mobile_name" 
        			Class="ColumnText" 
        			FieldName="mobile_name" 
        			Label=" Name" 
        			MaxLength="15"        			 
        			Sortable="Y"
        			 />        			
        	
   
    
		<Element Name="fld_mobile" 
        			Class="ColumnText" 
        			FieldName="mobile" 
        			Label="mobile" 
        			MaxLength="15"        			 
        			Sortable="Y"
        			 />        			
        	
   
    
		<Element Name="fld_subject" 
        			Class="ColumnText" 
        			FieldName="subject" 
        			Label="Subject" 
        			MaxLength="15"        			 
        			Sortable="Y"
        			 />        			
        	
   
    
		<Element Name="fld_mobile" 
        			Class="ColumnText" 
        			FieldName="mobile" 
        			Label="Mobile" 
        			MaxLength="15"        			 
        			Sortable="Y"
        			 />        			
        	
   
    
    
    
    
    
  
    </DataPanel>
    <ActionPanel /> 
    <NavPanel />     
</EasyForm>
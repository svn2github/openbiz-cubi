<BizDataObj Name="QueueDO" 
		Description="Sms Queue Description" 
		DataPermControl="N"
		Class="BizDataObj" 
		DBName="Default" 
		Table="sms_queue" 
		SearchRule="" 
		SortRule="[Id] DESC"
		OtherSQLRule=""
		Uniqueness="" 
		Stateless="N" 
		IdGeneration="Identity" 
		CacheLifeTime="0" 
		 >
	<BizFieldList>
		<BizField Name="Id"			
				Description="Id"	
				Column="id"    
				Required="N" 
				Type="Number"/>
				

		<BizField Name="mobile"		
				Description="mobile"	
				Column="mobile" 
				Type="Text"
				Required="N" 
				 />
		<BizField Name="content"		
				Description="Content"	
				Column="content" 
				Type="Text"
				Required="N" 
				 />
		<BizField Name="status"		
				Description="Status"	
				Column="status" 
				Type="Text"
				Required="N" 
				 />
		<BizField Name="schedule"							
				Column="schedule" 											 
				 />			 			 
		<BizField Name="create_by"	
				Description="Create By"	
				Column="create_by"	
				Type="Number"	
				ValueOnCreate="{@profile:Id}"/>
		<BizField Name="create_time"	
				Description="Create Time"	
				Column="create_time"	
				Type="Datetime"	
				ValueOnCreate="{date('Y-m-d H:i:s')}"/>
		<BizField Name="update_time"	
				Description="Update Time"	
				Column="update_time"	
				Type="Datetime"	
				ValueOnCreate="{date('Y-m-d H:i:s')}"	
				ValueOnUpdate="{date('Y-m-d H:i:s')}"/>				
	</BizFieldList>
	<TableJoins>
				    	
	</TableJoins>
	<ObjReferences>
	
	</ObjReferences>
</BizDataObj>
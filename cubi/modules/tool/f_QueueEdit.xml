<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="f_QueueEdit" Class="F_ElementEdit" FormType="Edit" jsClass="jbForm" Title="" Description="" BizDataObj="" DefaultForm="Y" TemplateEngine="Smarty" TemplateFile="detail.tpl">
    <DataPanel>
        <Element Name="fld_name" Class="InputText" FieldName="Name" Label="Name" Width="200"/>
        <Element Name="fld_source" Class="tool.EtlListbox" FieldName="source" Label="Source" SelectFrom="dbs()" Width="200"/>
        <Element Name="fld_dest" Class="tool.EtlListbox" FieldName="destination" Label="Destination" SelectFrom="dbs()" Width="200"/>
    </DataPanel>
    <ActionPanel>
        <Element Name="btn_save" Class="Button" Text="Save " CssClass="button_gray_m">
            <EventHandler Name="save_onclick" Event="onclick" function="SaveRecord()"/>
        </Element>
        <Element Name="btn_reset" Class="Button" Text="Reset" CssClass="button_gray_m">
            <EventHandler Name="reset_onclick" Event="onclick" function="js:$('{@:m_Name}').reset()"/>
        </Element>
        <Element Name="html_msg" Class="HTMLBlock" text="" style="color:gray; padding:2"/>
    </ActionPanel> 
    <NavPanel>
    </NavPanel> 
    <SearchPanel>
    </SearchPanel>
</EasyForm>

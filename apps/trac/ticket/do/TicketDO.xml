<?xml version="1.0" standalone="no"?>
<BizDataObj Name="TicketDO" Description="" Class="BizDataObj" DBName="Default" Table="trac_ticket" SearchRule="" SortRule="" OtherSQLRule="" Uniqueness="" Stateless="N" IdGeneration="Identity" AccessRule="" CacheLifeTime="0">
	<BizFieldList>
   		<BizField Name="Id" Column="id" Type="Number"/>

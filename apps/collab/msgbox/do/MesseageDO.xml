<?xml version="1.0" standalone="no"?>
<BizDataObj Name="MesseageDO" Description="" Class="BizDataObj" DBName="" Table="messeage" SearchRule="[from_userid]='{@profile:Id}'" SortRule="" OtherSQLRule="" Uniqueness="" Stateless="N" IdGeneration="Identity" CacheLifeTime="7200">

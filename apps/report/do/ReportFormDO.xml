<?xml version="1.0" standalone="no"?>
<BizDataObj Name="ReportFormDO" Description="" Class="BizDataObj" DBName="Default" Table="report_form" SearchRule="" SortRule="[sortorder] ASC" OtherSQLRule="" Uniqueness="title,view_id" Stateless="N" IdGeneration="Identity" CacheLifeTime="0">
